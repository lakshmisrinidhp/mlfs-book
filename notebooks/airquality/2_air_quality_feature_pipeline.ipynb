   "source": [
    "for _, sensor in sensors_df.iterrows():\n",
    "    air_quality_fg = fs.get_feature_group(\n",
    "        name=sensor[\"feature_group_name\"],\n",
    "        version=1,\n",
    "    )\n",
    "\n",
    "weather_fg = fs.get_feature_group(\n",
    "    name='weather',\n",
    "    version=1,\n",
    ")\n"
