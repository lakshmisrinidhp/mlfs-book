    "# use Hyderabad city coordinates once (e.g., first row in the CSV)\n",
    "city_name = sensors_df.iloc[0][\"city\"]\n",
    "lat = sensors_df.iloc[0][\"latitude\"]\n",
    "lon = sensors_df.iloc[0][\"longitude\"]\n",
    "\n",
    "hourly_df = util.get_hourly_weather_forecast(city_name, lat, lon)\n",
    "hourly_df = hourly_df.set_index('date')\n",
    "\n",
    "daily_df = hourly_df.between_time('11:59', '12:01')\n",
    "daily_df = daily_df.reset_index()\n",
    "daily_df['date'] = pd.to_datetime(daily_df['date']).dt.date\n",
    "daily_df['date'] = pd.to_datetime(daily_df['date'])\n",
    "daily_df['city'] = city_name\n",
    "daily_df\n"
   ]
