    "import pandas as pd\n",
    "from pathlib import Path\n",
    "import json\n",
    "\n",
    "project = hopsworks.login(project="Scalable_ML_DL_2026")\n",
    "fs = project.get_feature_store() \n",
    "secrets = hopsworks.get_secrets_api()\n",
    "\n",
    "AQICN_API_KEY = secrets.get_secret(\"AQICN_API_KEY\").value\n",
    "\n",
    "\n",
    "SENSORS_CSV = Path(root_dir) / \"data\" / \"hyderabad_sensors_pipeline.csv\"\n",
    "sensors_df = pd.read_csv(SENSORS_CSV)\n",
    "\n",
    "today = datetime.date.today()\n",
    "\n",
    "for _, sensor in sensors_df.iterrows():\n",
    "    country = sensor[\"country\"]\n",
    "    city = sensor[\"city\"]\n",
    "    street = sensor[\"street_display_name\"]\n",
    "    aqicn_url = sensor[\"aqicn_url\"]\n",
    "    latitude = sensor[\"latitude\"]\n",
    "    longitude = sensor[\"longitude\"]\n",
    "    feature_group_name = sensor[\"feature_group_name\"]\n",
    "\n",
    "    # now use country/city/street/aqicn_url/lat/lon to fetch today‚Äôs PM2.5,\n",
    "    # insert into fs.get_feature_group(feature_group_name), etc.\n"
   ]
  },
